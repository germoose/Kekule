package bitvectors;
import java.util.Arrays;
import java.util.Set;
import java.util.TreeSet;

/**
 * 
 * @author Aaron
 *
 */
public class Utils {
	
	/**
	 * 
	 * @param unsorted
	 * @return
	 */
	public static String sort(String unsorted){
		char[] chars = unsorted.toCharArray();
		Arrays.sort(chars);
		return new String(chars);
	}
	
}

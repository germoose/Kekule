package bitvectors;

public class BiInt {
	/**
	 * Represents a binary integer
	 */
	private int number;
	
	public BiInt(int n){
		this.number = n;
	}
	
	public String toString(){
		return this.number + " " + "(" + Integer.toBinaryString(number) + ")";
	}

	public int getNumber() {
		return number;
	}

	public void setNumber(int number) {
		this.number = number;
	}
}
